<h2>All Historical Landmarks</h2>

<table>
  <th>Name</th>
  <th>Year Completed</th>
  <tr>
    <% @landmarks.each do |landmark| %>
      <td align="center"><a href="/landmarks/<%= landmark.id %>"><%=landmark.name %></a>
      
      
      <ul>
        <% figure.titles.each do |title| %>
        <li><%=title.name%></li>
        <%end%>
      </ul>
      </td>
  </tr>
  <%end%>
</table>
<br>

<a class="button" href="/figures">Figures</a>
<a class="button" href="/">Home</a>
